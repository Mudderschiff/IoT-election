    \entry{eg-paper}{inproceedings}{}{}
      \name{author}{4}{}{%
        {{hash=14a2b78ffb98c1cb4013dabb4ae9f075}{%
           family={Benaloh},
           familyi={B\bibinitperiod},
           given={Josh},
           giveni={J\bibinitperiod}}}%
        {{hash=b1315d1cb133620e9488e665512a2272}{%
           family={Naehrig},
