    \entry{serialisation-comparison}{article}{}{}
      \name{author}{4}{}{%
        {{hash=bf02db4b7da33ffa8c235edb623f02f8}{%
           family={Luis},
           familyi={L\bibinitperiod},
           given={Álvaro},
           giveni={Á\bibinitperiod}}}%
        {{hash=5f02eb5c056cd075e544ce61c1553721}{%
           family={Casares},
